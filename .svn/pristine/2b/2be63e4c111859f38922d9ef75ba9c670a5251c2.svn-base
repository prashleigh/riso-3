                var self = $(this);
                self.css({ 'background-color': 'white', 'color': 'black' });

                switch (self.text()) {
                    case "Artwork":
                    case "Associated Media":
                        dropInk.hide();
                        dropFile.hide();
                        break;
                    case "From File":
                        dropFile.show();
                        dropInk.hide();
                        break;
                    case "Audio (MP3)":
                    case "Video (MP4, WEBM, OGV)":
                    case "Image":
