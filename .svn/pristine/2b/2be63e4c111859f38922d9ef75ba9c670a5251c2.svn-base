            if (needRefresh) {
                ctime = timeManager.timeToPx(time);
            }

            else if (player && !playing) {
                stop();
                playing = false;
                player.pause(time);
            }
        }
    }
    that.seek = seek;
    timeManager.onSeek(function (ev) { seek(ev.current); });

