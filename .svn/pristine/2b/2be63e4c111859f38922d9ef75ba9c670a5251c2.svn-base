                trackBody.scrollTop(newAbsoluteScroll);
                trackTitleWrapper.scrollTop(newAbsoluteScroll);
                cancelAccel();
            }
        });
        
        elements.push(widget);
        elements.push(verticalScroller);

        verticalScroller.mousedown(function (evt) {
