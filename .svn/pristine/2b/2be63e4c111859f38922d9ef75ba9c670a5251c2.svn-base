    function createConversionLoading(msg, nocircle, isMp4) {
        var container = $(document.createElement('div'));
        container.attr('id', 'leftLoading');
        container.css({
            'position': 'absolute',
            'width': '80%',
            'left': '10%',
            'top': '40%',
