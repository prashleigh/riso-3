	 */
	function randElt(arr) {
		var ind;
		console.log("arr.length = "+arr.length);
		if(arr.length === 0) {
			return null;
		} else {
			ind = randIndex(arr);
			console.log('index = '+ind);
			return arr[ind];
		}
	}

