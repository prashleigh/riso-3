    var debounce = $.debounce(200, coreUpdate);
    function coreUpdate() {
        onUpdateNumCalls = onUpdateNumCalls + 1;

        timeManager.stop();

        var rin;
        if (loaded) {
            viewer.setIsReloading(true);
