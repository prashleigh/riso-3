        if (inArtworkView) {
            loadArtView();
        } else if (inAssociatedView) {
            loadAssocMediaView();
        } else if (inCollectionsView) {
            loadExhibitionsView();
        } else if (inFeedbackView) {
            loadFeedbackView();
        } else if (inToursView) {
            loadTourView();
        }
    }

