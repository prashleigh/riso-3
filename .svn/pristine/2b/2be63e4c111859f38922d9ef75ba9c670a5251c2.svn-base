                    data[i][5] = translation;
                    data[i][6] = loc;
                    data[i][7] = offset;
                    data[i][8] = fadeinrightbound;
                    data[i][9] = fadeoutleftbound;
                }        
            }
        }
        currentDisplay.msMove(selectDisplays, translation);//move all selectedDisplays
    }
