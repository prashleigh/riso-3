            });
        } else {
            verticalScroller.draggable("enable");
            sliderPane.show();
            verticalScroller.css({
                'height': (trackTitleWrapper.height() - 4) * sliderPane.height() / calculateTotalTrackHeight() + 'px',
            });
        }
    }
