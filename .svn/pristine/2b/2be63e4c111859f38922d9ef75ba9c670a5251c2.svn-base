        switch (newViewState) {
            case viewStates.snapped:
            case viewStates.filled:
                if (!splitOverlay)
                    $("body").append(splitOverlay = new LADS.Layout.MetroSplitscreenMessage().getRoot());
                break;
            case viewStates.fullScreenLandscape:
