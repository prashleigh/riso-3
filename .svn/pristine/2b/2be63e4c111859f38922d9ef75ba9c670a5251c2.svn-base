         */
        function _addMenuItem(title, component, id) {
            var item = $(document.createElement('label'));
            item.addClass('optionItem');
            item.attr('id', id);
            item.text(title);
            item.css({
                "left": "0%",
                "position": "relative",
                "font-size": '.8em',//TAG.Util.getFontSize(170),
                "color": "rgb(256, 256, 256)",
