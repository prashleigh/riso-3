                        map.entities.get(l).setOptions({ icon: tagPath+'images/icons/locationPin.svg' });
                    }

                    if (custom) {
                        pushpin && pushpin.attr('src', tagPath+'images/icons/locationPin2.svg');
                    }
                    //if it's a bing map pushpin, it's set to red below

