        (!location.title && titleContainer.css({margin:'0px 0px 0px 10px'}));

        dateContainer.css({
            display: 'inline-block',
            margin: '0px 0px 0px 0px',
            position: 'relative',
            'vertical-align': 'middle',
            'max-font-size': '24px',
            'min-font-size' : "12px",
